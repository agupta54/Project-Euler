    "def fact(x):\n",
    "    if x == 1:\n",
    "        return x\n",
    "    else:\n",
    "        return x*fact(x-1)\n",
    "    \n",
    "n = 100\n",
    "\n",
    "m = fact(n)\n",
    "\n",
    "L = [int(i) for i in str(m)]\n",
    "\n",
    "print(sum(L))"
   ]
